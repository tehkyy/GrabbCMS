{
  "name": "grabbcms",
  "private": true,
  "version": "1.5.7",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "local": "vite --mode local",
    "prd": "vite --mode production",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "firebase use grabbit-370315 && yarn version --patch && yarn run build && firebase deploy --only hosting",
    "deploy-dev": "firebase use grabbit-dev-b598a && yarn version --patch && tsc && vite build --mode development && firebase deploy --only hosting"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@firecms/core": "^2.0.0",
    "@firecms/ui": "^2.0.0",
    "@mui/icons-material": "^5",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5",
    "@mui/x-date-pickers": "^5.0.0-beta.1",
    "@types/node": "^20.7.1",
    "axios": "^1.7.2",
    "date-fns": "2.21.1",
    "dotenv": "^16.3.1",
    "firebase": "^9",
    "firecms": "^2.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6",
    "react-router-dom": "^6",
    "tailwindcss": "^3.4.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
